,GAAA,CAAA,GAAA,CAAA,CAAA,QAAA,EAAA,GAAA,CAAA,CAlCA,CAkCA;;AACA,WAAA,OAAA,GAAA,OAAA;;AACA,UAAA,KAAA,CAAA,QAAA,KAAA,CAAA,IAAA,GAAA,CAAA,GAAA,GAAA,CAAA,CAAA,CAAA,QAAA,EAAA,EAAA;AAAA;AACA,YAAA,GAAA,IAAA,CAAA,EAAA;AACA,eAAA,SAAA,GAAA,OAAA,GAAA,CAAA;AACA,eAAA,SAAA,GAAA,KAAA,OAAA;AACA,SAHA,MAGA;AACA,eAAA,SAAA,GAAA,CAAA;AACA,eAAA,SAAA,GAAA,KAAA,OAAA,GAAA,CAAA;AACA;AACA,OARA,MAQA;AACA,aAAA,SAAA,GAAA,OAAA,GAAA,CAAA;AACA,aAAA,SAAA,GAAA,KAAA,OAAA;AACA;;AACA,UAAA,EAAA,GAAA,IAAA,IAAA,CAAA,KAAA,CAAA,WAAA,EAAA,EAAA,KAAA,CAAA,QAAA,EAAA,EAAA,CAAA,CAAA,CAhDA,CAgDA;;AACA,UAAA,EAAA,GAAA,IAAA,IAAA,CAAA,KAAA,CAAA,WAAA,EAAA,EAAA,KAAA,CAAA,QAAA,KAAA,CAAA,EAAA,CAAA,CAAA,CAjDA,CAiDA;;AACA,UAAA,IAAA,GAAA,KAAA,QAAA,CAAA,EAAA,EAAA,EAAA,CAAA,CAlDA,CAkDA;;AACA,WAAA,IAAA,GAAA,KAAA,QAAA,CAAA,EAAA,EAAA,KAAA,IAAA,CAAA,CAnDA,CAmDA;;AACA,UAAA,EAAA,CAAA,OAAA,KAAA,GAAA,CAAA,CAAA,CAAA,CAAA,OAAA,EAAA,GAAA,CAAA,EAAA;AAAA;AACA,aAAA,OAAA,GAAA,IAAA;AACA,OAFA,MAEA;AACA,aAAA,OAAA,GAAA,KAAA;AACA;;AACA,WAAA,MAAA,GAAA,IAAA,CAAA,KAAA,CAAA,CAAA,IAAA,GAAA,KAAA,IAAA,IAAA,IAAA,GAAA,GAAA,GAAA,OAAA,IAAA,OAAA,CAzDA,CAyDA;AAEA;AArEA,GAxBA;AA+FA,EAAA,OA/FA,qBA+FA;AACA,SAAA,YAAA;AACA;AAjGA,CAAA","sourcesContent":["<template>\n    <v-card class=\"elevation-0\">\n    <v-toolbar class=\"elevation-2\">\n    <v-toolbar-title>D-{{Math.floor(remainDays)}}</v-toolbar-title>\n    <v-spacer></v-spacer>\n    <v-toolbar-title>전역일 계산기</v-toolbar-title>\n    <v-spacer></v-spacer>\n    <v-toolbar-title id=\"title\">입대일: {{userInfo.days[0]}}<br/>전역일: {{userInfo.days[4]}}</v-toolbar-title>\n    </v-toolbar>\n    \n  <v-divider></v-divider>\n    <v-container fluid class=\"pt-5\">\n        <div>\n            <v-row>\n                <v-col lg=\"3\" md=\"3\" sm=\"6\" xs=\"12\" class=\"pb-2\">\n                    <v-card>\n                        <v-row class=\"no-gutters\">\n                            <div class=\"col-auto\">\n                                <div class=\"cyan fill-height\">&nbsp;</div>\n                            </div>\n                            <div class=\"col pa-3 py-4 cyan--text\">\n                                <h5 class=\"text-truncate text-uppercase\">현재 복무일</h5>\n                                <h1><animated-number\n                                    :value=\"currentDays\"\n                                    :formatValue=\"formatToDpDay\"\n                                    :duration=\"300\"\n                                    :round=\"1\"\n                                /></h1>\n                                <b>{{discharge_P}}%</b>\n                                <v-progress-linear \n                                :rotate=\"-90\"\n                                :size=\"100\" \n                                :width=\"7\" \n                                :value=\"discharge_P\" \n                                color=\"cyan\">\n\n                                </v-progress-linear>\n                            </div>\n                        </v-row>\n                    </v-card>\n                </v-col>\n                <v-spacer></v-spacer>\n                <v-col lg=\"3\" md=\"3\" sm=\"6\" xs=\"12\" class=\"pb-2\">\n                    <v-card>\n                        <v-row class=\"no-gutters\">\n                            <div class=\"col-auto\">\n                                <div class=\"primary fill-height\">&nbsp;</div>\n                            </div>\n                            <div class=\"col pa-3 py-4 primary--text\">\n                                <h5 class=\"text-truncate text-uppercase\">남은 복무일</h5>\n                                <h1><animated-number\n                                    :value=\"remainDays\"\n                                    :formatValue=\"formatToDmDay\"\n                                    :duration=\"300\"\n                                    :round=\"1\"\n                                /></h1>\n                                <b>{{remain_P}}%</b>\n                                <v-progress-linear \n                                :rotate=\"-90\"\n                                :size=\"100\" \n                                :width=\"7\" \n                                :value=\"remain_P\" \n                                color=\"primary\">\n                                </v-progress-linear>\n                            </div>\n                        </v-row>\n                    </v-card>\n                </v-col>\n                <v-spacer></v-spacer>\n                <v-col lg=\"3\" md=\"3\" sm=\"6\" xs=\"12\" class=\"pb-2\">\n                    <v-card>\n                        <v-row class=\"no-gutters\">\n                            <div class=\"col-auto\">\n                                <div class=\"success fill-height\">&nbsp;</div>\n                            </div>\n                            <div class=\"col pa-3 py-4 success--text\">\n                                <h5 class=\"text-truncate text-uppercase\">{{userInfo.classes[rankIdx+1]}} 진급</h5>\n                                <h1><animated-number\n                                    :value=\"promotion\"\n                                    :formatValue=\"formatToDmDay\"\n                                    :duration=\"300\"\n                                    :round=\"1\"\n                                /></h1>\n                                <b>{{promotion_P}}%</b>\n                                <v-progress-linear :rotate=\"-90\" :size=\"100\" :width=\"7\" :value=\"promotion_P\" color=\"success\"></v-progress-linear>\n                            </div>\n                        </v-row>\n                    </v-card>\n                </v-col>\n                <v-spacer></v-spacer>\n                <v-col v-show=\"(!nmHouse && userInfo.type=='병사')\" lg=\"3\" md=\"3\" sm=\"6\" xs=\"12\" class=\"pb-2\">\n                    <v-card>\n                        <v-row class=\"no-gutters\">\n                            <div class=\"col-auto\">\n                                <div class=\"error fill-height\">&nbsp;</div>\n                            </div>\n                            <div class=\"col pa-3 py-4 error--text\">\n                                <h5 class=\"text-truncate text-uppercase\" >{{userInfo.classes[nmRankIdx]}} {{nmStepIdx}}호봉 진급</h5>\n                                <h1><animated-number\n                                    :value=\"step\"\n                                    :formatValue=\"formatToDmDay\"\n                                    :duration=\"300\"\n                                    :round=\"1\"\n                                /></h1>\n                                <b>{{step_P}}%</b>\n                                <v-progress-linear :rotate=\"-90\" :size=\"100\" :width=\"7\" :value=\"step_P\" color=\"error\"></v-progress-linear>\n                            </div>\n                        </v-row>\n                    </v-card>\n                </v-col>\n            </v-row>\n        </div>\n    </v-container>\n    </v-card>\n</template>\n\n<style>\n    #title {\n        font-size: 0.75rem;\n    }\n    table th + th { border-left:1px solid #dddddd; }\n    table td + td { border-left:1px solid #dddddd; }\n</style>\n<script>\n  export default {\n    name: 'RetireCalculator',\n\n    data: () => ({\n    }),\n  }\n</script>\n\n<script>\nimport AnimatedNumber from \"animated-number-vue\";\n\nexport default {\n  components: {\n    AnimatedNumber\n  },\n  data() {\n    return {\n        userInfo : {},\n        tmp : [],\n        discharge_P : 0, //전역일 퍼센트\n        remain_P : 0, // 남은 복무일 퍼센트\n        promotion_P : 0, // 다음 계급 진급일 퍼센트\n        step_P : 0,     // 다음 호봉 진급일 퍼센트\n        currentDays : 0, // 현재 복무일\n        remainDays : 0,  // 남은 복무일\n        promotion : 0,   // 다음 계급 진급일\n        step : 0,       // 다음 호봉 진급일\n        serviceDays : 0, // 전체 복무일\n        rankIdx : 0,      // 사용자의 현재 계급idx\n        nmRankIdx :0,      //사용자의 현재 계급idx\n        stepIdx : 0,      // 사용자의 현재 호봉\n        nmStepIdx : 0,     // 사용자음 다음 호봉\n        nmHouse : false,   // 다음 달에 전역일 경우\n    };\n  },\n  methods: {\n    dateDiff(a,b){\n      return (a.getTime()-b.getTime())/(1000*3600*24);\n    },\n    formatToDmDay(value) {\n      return `D-${value}`;\n    },\n    formatToDpDay(value) {\n      return `D+${value}`;\n    },\n    updateRcData(){\n        var ui = {\n            days : [\"2020-08-24\",\"2020-11-01\",\"2021-05-01\",\"2021-11-01\",\"2022-02-23\"],       \n            classes : [\"이등병\",\"일병\",\"상병\",\"병장\",\"민간인\"],\n            type : \"병사\",\n        }; this.userInfo = ui;\n        var tmp = []; //진급일 배열을 임시로 저장할 배열\n        console.log(tmp);\n        var idx = 0;  // 사용자의 현재 계급의 인덱스\n        var today = new Date(); \n        ui.days.forEach((item)=> {         //tmp를 date객체를 가진 배열로 만들어 준다.\n            var list = item.split(\"-\");\n            tmp.push(new Date(list[0],list[1]-1,list[2]));\n        });\n        var all = Math.floor(this.dateDiff(tmp[4],tmp[0]));//총 복무일\n        this.serviceDays = all;       \n        var cur = Math.floor(this.dateDiff(today,tmp[0])); //현재 복무일\n        this.currentDays = cur;\n        this.discharge_P = Math.round((this.dateDiff(today,tmp[0]) / all *100)*1000000)/1000000; //전역일 퍼센트\n        this.remain_P = Math.round((100-this.discharge_P)*1000000)/1000000.0;\n        /*----------------------------------------------------------------------------------------*/\n        this.remainDays = (all - cur);                     //남은 복무일\n        for(var i=0;i<4;i++){ //현재 계급 구하기\n            if(tmp[i].getTime()<=today.getTime() && tmp[i+1].getTime()>today.getTime()){\n            idx = i;\n            break;\n            }\n        }\n        this.rankIdx = idx;\n        var prom = this.dateDiff(tmp[idx+1],today)+1; //다음 계급 진급일\n        this.promotion = prom;\n        var prodiff = this.dateDiff(tmp[idx+1],tmp[idx]) // 현재 계급 진급일과 다음 계급 진급일 일수 차 구하기\n        this.promotion_P = Math.round(((prodiff-prom)/prodiff *100)*1000000)/1000000; // 다음계급 진급일 퍼센트\n            /*----------------------------------------------------------------------------------------*/\n        var curStep = today.getMonth() - tmp[idx].getMonth() +1;  // 현재 호봉 수\n        this.stepIdx = curStep;\n        if(today.getMonth()+1 == tmp[idx+1].getMonth()){ //다음달이 진급일이라 호봉 수가 1로 바뀔 때\n            if(idx == 3){\n            this.nmStepIdx = curStep+1;\n            this.nmRankIdx = this.rankIdx;\n            }else{\n            this.nmStepIdx = 1;\n            this.nmRankIdx = this.rankIdx +1;\n            }\n        } else {        \n            this.nmStepIdx = curStep+1;\n            this.nmRankIdx = this.rankIdx;\n        }\n        var tm = new Date(today.getFullYear(),today.getMonth(),1);    //현재 달의 첫날\n        var nm = new Date(today.getFullYear(),today.getMonth()+1,1);  //다음 달의 첫날\n        var diff = this.dateDiff(nm,tm)       //한달 크기 구하기\n        this.step =  this.dateDiff(nm,today)+1;            //다음 호봉 진급일\n        if((nm.getTime()-tmp[4].getTime()) > 0 ){                      //다음 달 첫날이 전역 이후 일 때\n            this.nmHouse= true;\n        }else {\n            this.nmHouse = false;\n        }\n        this.step_P = Math.round(((diff-this.step)/diff *100)*1000000)/1000000; // 다음 호봉 진급일 퍼센트\n        \n        }\n    },\n    mounted() {\n      this.updateRcData();\n    },\n};\n</script>"],"sourceRoot":"src/components"}]}